			}
		},
		{
			"scope": [
				"keyword",
				"keyword.operator.new",
				"storage",
				"punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "{syntax-keywords-color}"
			}
		},
		{
			"scope": [
				"keyword.operator",
				"punctuation",
				"meta.selector.pseudo-class",
				"entity.other.attribute-name.pseudo-class"
			],
			"settings": {
				"foreground": "{syntax-operators-color}"
			}
		},
		{
			"scope": [
				"constant"
			],
			"settings": {
				"foreground": "{syntax-constants-color}"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string.other.link.title.markdown",
				"meta.at-rule.apply.tailwind"
			],
			"settings": {
				"foreground": "{syntax-strings-color}"
			}
		},
		{
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"markup.fenced_code.block.markdown"
			],
			"settings": {
				"foreground": "{syntax-comments-color}"
			}
		},
		{
			"scope": [
				"meta.selector",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "{syntax-classes-color}"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "{syntax-interfaces-color}"
			}
		},
		{
			"scope": [
				"meta.function"
			],
			"settings": {
				"foreground": "{syntax-methods-color}"
			}
		},
		{
			"scope": "markup.italic.markdown",
			"settings": {
				"foreground": "{syntax-strings-color}",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.strikethrough.markdown",
			"settings": {
				"foreground": "{syntax-variables-color}",
				"fontStyle": "strikethrough"
			}
		},
		{
			"scope": "markup.underline.link",
			"settings": {
				"foreground": "{syntax-methods-color}",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold.markdown",
			"settings": {
				"foreground": "{syntax-interfaces-color}",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.heading.markdown",
				"meta.separator.markdown"
			],
			"settings": {
				"foreground": "{syntax-interfaces-color}",
				"fontStyle": "bold"
